	<!-- SL Logo -->
		<Layer
			Condition="Color() < 13"
			File="SimplyLove" 
			InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" 
			OnCommand="%function(self) if love == 0 then self:diffusealpha(0) self:linear(0.4) self:diffusealpha(1) end end"
			OffCommand="%TitleMenuOut"
		/>
	<!-- Theme 13 logo -->
		<Layer
			Condition="Color() == 13"
			File="../ScreenLove underlay/A" 
			InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoom,.5;diffusealpha,0.5" 
			OnCommand="%function(self) if love == 0 then self:diffusealpha(0) self:linear(0.4) self:diffusealpha(0.5) end end"
			OffCommand="%TitleMenuOut"
		/>
	<!-- Number of Songs available -->
		<Layer 
			Font="_misoreg small" 
			Text="" 
			Command="%function(self)
				if Color() < 13 then
					self:y(SCREEN_CENTER_Y-126);
					self:shadowlength(0);
					self:zoom(0.45);
				else
					self:y(SCREEN_CENTER_Y-178);
					self:shadowlength(1);
					self:zoom(0.55);
				end
				self:x(SCREEN_CENTER_X);
				self:diffusealpha(0);
				self:sleep(.3);
				self:queuecommand('Set');
			end" 
			SetCommand="%function(self) 
				s = SCREENMAN:GetTopScreen():GetChild('Songs'):GetText() 
					self:settext(s) 
					self:linear(.5) 
					self:diffusealpha(1) 
				Command = {} function file() 
					if children then 
						File=children[math.mod(table.getn(Command),table.getn(children))+1]
					end 
				end 
			end" 
			OffCommand="%TitleMenuOut" 
		/>
	<!-- Life Difficulty -->
		<Layer 
			Font="_misoreg small" 
			Text="" 
			Command="%function(self)
				if Color() < 13 then
					self:y(SCREEN_CENTER_Y-143);
					self:zoom(0.45);
					self:shadowlength(0);
				else
					self:y(SCREEN_CENTER_Y-198);
					self:zoom(0.55);
					self:shadowlength(1);
				end
				self:x(SCREEN_CENTER_X);
				self:diffusealpha(0);
				self:sleep(.3);
				self:queuecommand('Set')
			end" 
			SetCommand="%function(self) 
				s = SCREENMAN:GetTopScreen():GetChild('LifeDifficulty'):GetText() 
					self:settext(s) 
					self:linear(.5) 
					self:diffusealpha(1) _G['mo'..'dL'..'ist']='ran'..'do'..'mva'..'ni'..'sh' 
			end" 
			OffCommand="%TitleMenuOut" 
		/>
