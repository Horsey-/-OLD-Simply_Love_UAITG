			OnCommand="zoom,0.85;pulse;EffectMagnitude,0.85,0.90,.5"
			Condition="Color() == 13"
		>
			<children>
				<Actor
					File="s.txt"
					OnCommand="x,-25"
				/>
				<Actor
					File="smooth minus.txt"
					OnCommand="x,30"
				/>
			</children>
		</ActorFrame>
	<!-- SL Grade -->
		<ActorFrame
			OnCommand="zoom,0.85"
			Condition="Color() < 13"
		>
			<children>
				<Actor
					File="s-minus.png"
				/>
			</children>
		</ActorFrame>
	</children>
</ActorFrame>
